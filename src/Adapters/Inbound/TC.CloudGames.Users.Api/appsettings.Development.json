{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MessageBroker": {
    "Type": "RabbitMQ"
  },
  "Cache": {
    "Host": "localhost",
    "Port": 56379,
    "Password": "Redis@123",
    "InstanceName": "TC.CloudGames.Users.Api:",
    "Secure": false
  },
  "RabbitMq": {
    "Host": "localhost",
    "Port": 55672,
    "VirtualHost": "/",
    "UserName": "guest",
    "Password": "guest",
    "Exchange": "user.events",
    "AutoProvision": true,
    "Durable": true,
    "UseQuorumQueues": false,
    "AutoPurgeOnStartup": false
  },
  "AzureServiceBus": {
    "ConnectionString": "Endpoint=sb://<namespace>.servicebus.windows.net/;SharedAccessKeyName=<key-name>;SharedAccessKey=<key-value>",
    "TopicName": "user.events",
    "AutoProvision": true,
    "MaxDeliveryCount": 10,
    "EnableDeadLettering": true,
    "AutoPurgeOnStartup": false,
    "UseControlQueues": true
  },
  "Database": {
    "Host": "localhost",
    "Port": 65432,
    "Name": "tc-cloudgames-users-db",
    "MaintenanceDatabase": "postgres",
    "User": "postgres",
    "Password": "postgres",
    "Schema": "public",
    "ConnectionTimeout": 30
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "TC.CloudGames.Users.Api"
    }
  }
}
