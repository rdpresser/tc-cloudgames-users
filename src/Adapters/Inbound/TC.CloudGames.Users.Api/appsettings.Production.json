{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MessageBroker": {
    "Type": "AzureServiceBus"
  },
  "AzureServiceBus": {
    "ConnectionString": "<your-azure-service-bus-connection-string>",
    "TopicName": "user-events",
    "Durable": true
  },
  "Database": {
    "Host": "<prod-db-host>",
    "Port": 5432,
    "Name": "tc_cloudgames_user",
    "MaintenanceDatabase": "postgres",
    "User": "<db-user>",
    "Password": "<db-password>",
    "Schema": "public"
  }
}